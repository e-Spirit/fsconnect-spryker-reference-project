<?xml version="1.0" encoding="UTF-8"?>
<CMS_MODULE>

  <CMS_INPUT_CHECKBOX name="wf_releasewmedia" gridHeight="1" gridWidth="1" useLanguages="no">
    <ENTRIES>
      <ENTRY value="true">
        <LANGINFOS>
          <LANGINFO lang="*" label="Release including media"/>
          <LANGINFO lang="DE" label="Freigabe inkl. Medien"/>
        </LANGINFOS>
      </ENTRY>
    </ENTRIES>
    <LANGINFOS>
      <LANGINFO lang="*" label="Release options"/>
      <LANGINFO lang="DE" label="Freigabeoptionen"/>
    </LANGINFOS>
  </CMS_INPUT_CHECKBOX>

  <CMS_INPUT_CHECKBOX name="wf_releaseRecursively" gridHeight="1" gridWidth="1" useLanguages="no">
    <ENTRIES>
      <ENTRY value="true">
        <LANGINFOS>
          <LANGINFO lang="*" label="Release recursively including child elements"/>
          <LANGINFO lang="DE" label="Rekursive Freigabe inkl. Kindelementen"/>
        </LANGINFOS>
      </ENTRY>
    </ENTRIES>
    <LANGINFOS>
      <LANGINFO lang="*" label="Advanced Release options"/>
      <LANGINFO lang="DE" label="Erweiterte Freigabeoptionen"/>
    </LANGINFOS>
  </CMS_INPUT_CHECKBOX>

</CMS_MODULE>